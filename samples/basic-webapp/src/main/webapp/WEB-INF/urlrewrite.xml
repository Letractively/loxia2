<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite
    PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN"
    "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">

<urlrewrite>
    <rule>
       <from>^/commons/userportrait/([0-9]+)\.jpg$</from>
       <to type="redirect" last="true">%{context-path}/commons/getattachment.do?userInfoIdForPortrait=$1</to>
    </rule>
</urlrewrite>